{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Docusaurus v2",
  "docs": "https://v2.docusaurus.io/docs/typescript-support",
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["DOM"],
    "moduleResolution": "node",
    "noEmit": true,
    "types": ["node", "@docusaurus/module-type-aliases", "@docusaurus/theme-classic", "docusaurus-plugin-sass"],
    "baseUrl": ".",
    "resolveJsonModule": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@site/*": ["./*"],
      "@edim-editor/tables": ["../preset-tables/src/index.ts"],
      "@edim-editor/emoji": ["../preset-emoji/src/index.ts"],
      "@edim-editor/heading": ["../preset-heading/src/index.ts"],
      "@edim-editor/command": ["../preset-command/src/index.ts"],
      "@edim-editor/hr": ["../preset-hr/src/index.ts"],
      "@edim-editor/paragraph": ["../preset-paragraph/src/index.ts"],
      "@edim-editor/flat-list": ["../preset-flat-list/src/index.ts"],
      "@edim-editor/flat-task-list": ["../preset-flat-task-list/src/index.ts"],
      "@edim-editor/mention": ["../preset-mention/src/index.ts"],
      "@edim-editor/image": ["../preset-image/src/index.ts"],
      "@edim-editor/link": ["../preset-link/src/index.ts"],
      "@edim-editor/core": ["../preset-core/src/index.ts"],
      "@edim-editor/codeblock": ["../preset-codeblock/src/index.ts"],
      "@edim-editor/blockquote": ["../preset-blockquote/src/index.ts"],
      "@edim-editor/bold": ["../preset-bold/src/index.ts"],
      "@edim-editor/code": ["../preset-code/src/index.ts"],
      "@edim-editor/font-family": ["../preset-font-family/src/index.ts"],
      "@edim-editor/italic": ["../preset-italic/src/index.ts"],
      "@edim-editor/strikethrough": ["../preset-strikethrough/src/index.ts"],
      "@edim-editor/subscript": ["../preset-subscript/src/index.ts"],
      "@edim-editor/superscript": ["../preset-superscript/src/index.ts"],
      "@edim-editor/text-color": ["../preset-text-color/src/index.ts"],
      "@edim-editor/underline": ["../preset-underline/src/index.ts"],
      "@edim-editor/ui": ["../preset-ui/src/index.ts"],
      "@edim-editor/menubar": ["../preset-menubar/src/index.ts"]
    },
    "skipLibCheck": true
  }
}
