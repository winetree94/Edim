/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "lib": [
      "ES2022",
      "dom"
    ],
    "paths": {
      "prosemirror-preset-document": ["../preset-document/src/index.ts"],
      "prosemirror-preset-tables": ["../preset-tables/src/index.ts"],
      "prosemirror-preset-emoji": ["../preset-emoji/src/index.ts"],
      "prosemirror-preset-heading": ["../preset-heading/src/index.ts"],
      "prosemirror-preset-command": ["../preset-command/src/index.ts"],
      "prosemirror-preset-hr": ["../preset-hr/src/index.ts"],
      "prosemirror-preset-history": ["../preset-history/src/index.ts"],
      "prosemirror-preset-paragraph": ["../preset-paragraph/src/index.ts"],
      "prosemirror-preset-flat-list": ["../preset-flat-list/src/index.ts"],
      "prosemirror-preset-mention": ["../preset-mention/src/index.ts"],
      "prosemirror-preset-italic": ["../preset-italic/src/index.ts"],
      "prosemirror-preset-image": ["../preset-image/src/index.ts"],
      "prosemirror-preset-link": ["../preset-link/src/index.ts"],
      "prosemirror-preset-core": ["../preset-core/src/index.ts"],
      "prosemirror-preset-codeblock": ["../preset-codeblock/src/index.ts"],
      "prosemirror-preset-blockquote": ["../preset-blockquote/src/index.ts"],
      "prosemirror-preset-strikethrough": ["../preset-strikethrough/src/index.ts"],
      "prosemirror-preset-strong": ["../preset-strong/src/index.ts"],
      "prosemirror-preset-keymap": ["../preset-keymap/src/index.ts"],
      "prosemirror-preset-text": ["../preset-text/src/index.ts"],
      "prosemirror-preset-text-color": ["../preset-text-color/src/index.ts"],
      "prosemirror-preset-code": ["../preset-code/src/index.ts"],
      "prosemirror-preset-ui": ["../preset-ui/src/index.ts"],
      "prosemirror-preset-menubar": ["../preset-menubar/src/index.ts"],
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
